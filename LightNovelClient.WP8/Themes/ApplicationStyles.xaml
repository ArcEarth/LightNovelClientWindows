    <Style x:Key="RibbonIconCirclebackground" TargetType="ContentControl">
        <Setter Property="Width" Value="50"></Setter>
        <Setter Property="Height" Value="50"></Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Viewbox Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Stretch="Fill">
                        <Canvas Width="76" Height="76">
                            <Ellipse Width="60" Height="60" Fill="{TemplateBinding Background}" Canvas.Left="8" Canvas.Top="8"/>
                            <Path Width="19" Height="38" Canvas.Left="28.5" Canvas.Top="19" Stretch="Fill" Fill="{TemplateBinding Foreground}" Data="F1 M 28.5,57L 28.5,19L 47.5,19L 47.5,57L 38,47.5L 28.5,57 Z "/>
                        </Canvas>
                    </Viewbox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
